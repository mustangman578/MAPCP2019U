a = np.array([1,1,1])

np.diag(a)
Out[84]: 
array([[1, 0, 0],
       [0, 1, 0],
       [0, 0, 1]])

2*np.diag(a)
Out[85]: 
array([[2, 0, 0],
       [0, 2, 0],
       [0, 0, 2]])

a = np.eye(3)

a
Out[87]: 
array([[1., 0., 0.],
       [0., 1., 0.],
       [0., 0., 1.]])

2*a
Out[88]: 
array([[2., 0., 0.],
       [0., 2., 0.],
       [0., 0., 2.]])


